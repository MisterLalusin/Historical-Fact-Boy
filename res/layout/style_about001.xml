<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/style_about001"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="pro.gr.ams.historicalfactboy.about001"
    android:background="@raw/about_background002">

    <WebView
        android:id="@+id/webView1"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/webView2" />

    <WebView
        android:id="@+id/webView2"
        android:layout_width="fill_parent"
        android:layout_height="55dp"
        android:scrollbars="none"
        android:layout_alignBottom="@+id/imageView6"
        android:layout_alignParentStart="true" >
    </WebView>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@raw/about_background001"
        android:id="@+id/imageView6"
        android:layout_marginStart="14dp"
        android:layout_marginTop="42dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="version 1.0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textSize="28dp"
        android:textAlignment="center"
        android:layout_marginTop="11dp"
        android:layout_alignTop="@+id/webView2"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
