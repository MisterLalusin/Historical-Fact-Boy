<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/style_menu001"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="pro.gr.ams.historicalfactboy.menu001"
    android:background="@raw/loading_background001">

    <ListView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:id="@+id/listView"
        android:background="@android:drawable/screen_background_light_transparent"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/header"
        android:layout_above="@+id/footer"
        android:textFilterEnabled="false" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:id="@+id/border"
        android:background="@raw/scorelogs_background001"
        android:layout_above="@+id/header"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:id="@+id/header"
        android:background="@android:drawable/screen_background_light_transparent"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:srcCompat="@raw/scorelogs_icon001"
        android:id="@+id/imageView"
        android:layout_marginLeft="80dp"
        android:layout_marginRight="80dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="25dp"
        android:layout_alignParentTop="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:id="@+id/footer"
        android:background="@raw/scorelogs_background001"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="65dp"/>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@raw/award_icon001"
        android:layout_alignTop="@+id/textView7"
        android:layout_toStartOf="@+id/textView7"
        android:id="@+id/imageView2"
        android:layout_marginBottom="10dp"/>

    <TextView
        android:text="Thank You For Playing!"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:id="@+id/textView7"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="normal|italic"
        android:textSize="18sp"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/imageView"
        android:layout_marginBottom="13dp"
        android:gravity="center_vertical" />

</RelativeLayout>
