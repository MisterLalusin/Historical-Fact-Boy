<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/style_developersoption001"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pro.gr.ams.historicalfactboy.developersoption001"
    android:background="@android:color/background_dark">


    <EditText
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/commandline"
        android:textAlignment="viewStart"
        android:hint="Result displays here"
        android:selectAllOnFocus="false"
        android:layout_alignParentStart="true"
        android:gravity="top"
        tools:inputType="textMultiLine"
        android:text="yakis.developer.start(method);"
        android:textColor="#0ea910"
        android:layout_marginTop="169dp"
        android:layout_marginBottom="-69dp"
        android:textAllCaps="false"
        android:textStyle="normal|italic"
        android:textSize="18sp" />


    <Button
        android:id="@+id/btninput"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:textAllCaps="false"
        android:textStyle="normal|italic"
        android:textColor="#0ea910"
        android:textSize="14sp"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:background="@raw/developersoptionheader001"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
