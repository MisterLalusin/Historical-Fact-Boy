<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/style_spanisheramultiplechooice002"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="pro.gr.ams.historicalfactboy.entirehistorymultiplechoiceseta002"
    android:background="@raw/score_background002">

    <TextView
        android:text="Highest Score:"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/activity_vertical_margin"
        android:background="@android:color/transparent"
        android:id="@+id/textHighScore"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="24sp"
        android:textAlignment="center"
        android:visibility="invisible"
        android:layout_alignTop="@+id/scorebox"
        android:layout_alignParentStart="true"
        android:layout_marginTop="92dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/textHighScore"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="55dp"
        android:orientation="vertical"
        android:id="@+id/linerarLayout">

        <TextView
            android:text="~~~~~~~~~~~~~~~~~~~\nScore: 0/100\n~~~~~~~~~~~~~~~~~~~"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:padding="@dimen/activity_vertical_margin"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textSize="20sp"
            android:textAlignment="center"
            android:id="@+id/textScore"/>

        <ViewFlipper
            android:layout_width="fill_parent"
            android:layout_height="230dp"
            android:layout_below="@+id/textView"
            android:layout_centerHorizontal="true"
            android:flipInterval="300"
            android:autoStart="true"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@raw/befactboy001"
                android:id="@+id/imageView1" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@raw/befactboy002"
                android:id="@+id/imageView2" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@raw/befactboy003"
                android:id="@+id/imageView3" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@raw/befactboy004"
                android:id="@+id/imageView4" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@raw/befactboy005"
                android:id="@+id/imageView5" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@raw/befactboy006"
                android:id="@+id/imageView6" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@raw/befactboy007"
                android:id="@+id/imageView7" />

        </ViewFlipper>

        <TextView
            android:text="~~~~~~~~~~~~~~~~~~~\nJust study history more.\n~~~~~~~~~~~~~~~~~~~"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:padding="@dimen/activity_vertical_margin"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textSize="20sp"
            android:textAlignment="center"
            android:id="@+id/textFeedback"/>

        <Button
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:id="@+id/tryagain"
            android:onClick="home001"
            android:background="@raw/back_button001"
            android:layout_gravity="center"
            android:visibility="visible"
            android:elevation="24dp"
            android:layout_alignBottom="@+id/textHighScore"
            android:layout_alignParentEnd="true" />

    </LinearLayout>


</RelativeLayout>